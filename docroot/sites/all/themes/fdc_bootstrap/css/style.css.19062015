


body {
	font-family: "news-gothic-std", "NewsGothic", "News Cycle", "Calibri", "Arial Narrow", "Geneva", "Helvetica", "Arial";
	font-size: 14px;
	color: #101010;
}
body.front { background-color: #444; }

h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
	font-family: 'Open Sans', sans-serif;
	font-weight: 800;
}

a,
.btn.btn-default { -webkit-transition: .2s ease-in-out; -moz-transition: .2s ease-in-out; -o-transition: .2s ease-in-out; transition: .2s ease-in-out; }

strong { font-weight: 700; }

.front .main-container.container { padding: 0; }
.page-news .main-container.container,
.node-type-news-articles .main-container.container { padding: 0 15px; }

.noscroll {overflow: hidden; position: fixed; }

.btn.btn-default {
	-moz-border-radius: 0;
	-webkit-border-radius: 0;
	border-radius: 0;
	border: 0 none;
	background: #ccc;
	font-weight: 800;
	text-transform: uppercase;
	padding: 10px 20px;
}
.btn.btn-default:hover {
	background: #fff;
}


