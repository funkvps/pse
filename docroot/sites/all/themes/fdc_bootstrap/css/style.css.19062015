    margin: 100px auto 0;
    width: 900px;
}
.overlay .close_white,
.overlay2 .close_white {
    background: url(../img/close_white.png) no-repeat scroll center center rgba(0, 0, 0, 0);
    cursor: pointer;
    float: right;
    height: 24px;
    margin: 50px;
    width: 24px;
    z-index: 999999;
}
.close_white {
    color: #000000;
    float: right;
    font-size: 21px;
    font-weight: bold;
    line-height: 1;
    -moz-text-shadow: 0 1px 0 #fff;
    -webkit-text-shadow: 0 1px 0 #fff;
    text-shadow: 0 1px 0 #fff;

}
.overlay .close_black,
.overlay2 .close_black {
    background:#3A4048 url(../img/close_white.png) no-repeat scroll center center ;
    cursor: pointer;
	position: absolute;
	top: 0;
	right: 0;
    height: 44px;
    margin: 0;
    width: 44px;
    z-index: 999999;
}
.close_black {
    color: #000000;
    float: right;
    font-size: 21px;
    font-weight: bold;
    line-height: 1;
